<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
    <%
        let title = '';
        if (is_post() && !is_page()) title = page.title + ' - ' + config.title;
        else if (!is_post()) title = config.title;
    %>
    <title><%= title %></title>
    <meta charset="utf-8">
    <% 
        let description = '';
        if (!is_page() && !is_post()) {
            description = config.description;
        } else {
            description = page.description;
        }
    %>
    <meta name="title" content="<%= title %>">
    <meta name="description" content="<%= description %>">
    <meta property="og:image" content="/<%= theme.favicon %>">
    <meta property="og:image:width" content="200" />
    <meta property="og:image:height" content="200" />
    <%- favicon_tag(theme.favicon) %>
    <% if (config.highlight.enable) { %>
    <link rel="stylesheet"
        href="//cdn.jsdelivr.net/gh/highlightjs/cdn-release@9.17.1/build/styles/arduino-light.min.css">
    <script src="//cdn.jsdelivr.net/gh/highlightjs/cdn-release@9.17.1/build/highlight.min.js"></script>
    <script>hljs.initHighlightingOnLoad();</script>
    <% } %>
    <%- css('css/style.css') %>
    <%- js('js/jquery.min.js') %>
    <%- js('js/saki.js') %>
</head>